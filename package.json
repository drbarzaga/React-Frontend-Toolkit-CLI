{
  "name": "react-frontend-toolkit-cli",
  "version": "0.2.2",
  "description": "A command-line tool designed to give developers quick access to a curated list of essential libraries and tools for frontend and React development, sourced from the React Frontend Toolkit repository",
  "main": "dist/index.js",
  "types": "types/types.d.ts",
  "bin": {
    "rft-cli": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "data/tools.json"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=6.0.0"
  },
  "readme": "README.npm.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/drbarzaga/React-Frontend-Toolkit-CLI"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest --config vitest.config.ts"
  },
  "keywords": [
    "react",
    "frontend",
    "toolkit",
    "cli",
    "command-line",
    "tool",
    "library",
    "development",
    "reactjs",
    "react.js",
    "react.js development",
    "react.js libraries",
    "react.js tools",
    "frontend development",
    "frontend libraries",
    "frontend tools"
  ],
  "author": "Dayan Perez <dayan.perez9012@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/figlet": "^1.7.0",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.8.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "dependencies": {
    "boxen": "^5.1.2",
    "chalk": "^4.0.0",
    "commander": "^12.1.0",
    "figlet": "^1.8.0",
    "inquirer": "^12.0.1"
  }
}
